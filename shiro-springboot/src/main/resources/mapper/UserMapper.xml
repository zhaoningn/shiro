<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dta/mybatis-3-mapper.dtd">
<mapper namespace="com.zhaoning.mapper.UserMapper">

    <select id="queryUserByName" parameterType="String" resultType="com.zhaoning.pojo.User">
        select * from mybatis.user where name = #{name}
    </select>
</mapper>